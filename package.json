{
	"name": "n8n-nodes-topfact-archiv",
	"version": "0.4.2",
	"description": "topfact Archiv Node for n8n",
	"license": "MIT",
	"homepage": "https://topfact.de",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "topfact AG",
		"email": "vertrieb@topfact.de"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/topfact/n8n-nodes-topfact-archiv.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release"
	},
	"files": [
